<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="350.0" style="-fx-background-color: lightgreen;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wonders.client.controller.trash.Dashboard">
   <children>
      <Label fx:id="player_name" alignment="CENTER" prefHeight="23.0" prefWidth="300.0" text="player_name">
         <font>
            <Font name="Agency FB Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="35.0" />
         </VBox.margin>
      </Label>
      <Button mnemonicParsing="false" onAction="#host_room" prefWidth="150.0" text="Host Room">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font name="Agency FB Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="join_room_btn" layoutX="72.0" layoutY="68.0" mnemonicParsing="false" onAction="#join_room" prefWidth="150.0" text="Join Room (00)">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="15.0" />
         </VBox.margin>
         <font>
            <Font name="Agency FB Bold" size="14.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="15.0" layoutY="45.0" prefHeight="23.0" prefWidth="300.0" text="Total Players">
         <font>
            <Font name="Agency FB Bold" size="21.0" />
         </font>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="total_players" alignment="CENTER" layoutX="10.0" layoutY="171.0" prefHeight="23.0" prefWidth="300.0" text="00">
         <font>
            <Font name="Agency FB Bold" size="80.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Button layoutX="35.0" layoutY="144.0" mnemonicParsing="false" onAction="#settings" prefWidth="150.0" text="Settings">
         <font>
            <Font name="Agency FB Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets top="40.0" />
         </VBox.margin>
      </Button>
      <Button layoutX="35.0" layoutY="133.0" mnemonicParsing="false" onAction="#help" prefWidth="150.0" text="Help">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="15.0" />
         </VBox.margin>
         <font>
            <Font name="Agency FB Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="35.0" layoutY="158.0" mnemonicParsing="false" onAction="#credit" prefWidth="150.0" text="Credit">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font name="Agency FB Bold" size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
